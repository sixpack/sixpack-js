{
  "name": "@jusbrasil/sixpack-client",
  "version": "2.3.5",
  "description": "JS client for SeatGeek's Sixpack AB testing framework.",
  "main": "sixpack.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "docker-compose up -d && SIXPACK_BASE_URL=http://localhost:8000 yarn test:mocha",
    "test:mocha": "mocha --globals document,window",
    "test:coverage": "nyc npm run test",
    "test:lcov": "nyc --reporter=lcov npm run test",
    "build:dev": "webpack --mode development --config webpack.config.js",
    "build:prod": "webpack --mode production --config webpack.config.js",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jusbrasil/sixpack-js.git"
  },
  "author": "SeatGeek",
  "license": "MIT",
  "readmeFilename": "README.md",
  "publishConfig": {
    "registry": "https://nexus.apps.jusbr.com/repository/npm-internal/"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-loader": "^8.2.2",
    "chai": "4.2.0",
    "jsdom": "^16.7.0",
    "mocha": "7.0.1",
    "nock": "13.0.5",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2"
  }
}
